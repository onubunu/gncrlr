<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>

    <% Product.find_each do |product| %>
      <tr>
        <td><%= link_to product.name, product %></td>
        <td><%= product.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>